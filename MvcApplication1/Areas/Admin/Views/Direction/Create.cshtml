@model SoftSchool.Models.dres

@{
    ViewBag.Title = "Create";
    Layout = Url.Content("~/Areas/Admin/Views/Shared/_Layout.cshtml");
}

<h2>Ajouter Direction Régionale</h2>

<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>

@using (Html.BeginForm()) {
    @Html.ValidationSummary(true)
    <table border="0" cellpadding="3" cellspacing="2">
    <tr><td width="100" valign="top" align="right">
        <div class="editor-label">
            @Html.LabelFor(model => model.Nom_Ar)
        </div>
        </td><td>
        <div class="editor-field">
            @Html.EditorFor(model => model.Nom_Ar)
            @Html.ValidationMessageFor(model => model.Nom_Ar)
        </div>
        </td></tr><tr><td align="right">
        <div class="editor-label">
            @Html.LabelFor(model => model.Nom_Fr)
        </div>
         </td><td>
        <div class="editor-field">
            @Html.EditorFor(model => model.Nom_Fr)
            @Html.ValidationMessageFor(model => model.Nom_Fr)
        </div>
        </td></tr><tr><td  align="right">
        <div class="editor-label">
            @Html.LabelFor(model => model.GovernoratID, "gouvernorats")
        </div>
        </td><td>
        <div class="editor-field">
            @Html.DropDownList("GovernoratID", String.Empty)
            @Html.ValidationMessageFor(model => model.GovernoratID)
        </div>
        </td></tr><tr><td>
        </td><td>
        <p>
            <button type="submit" class="btn btn-primary start">
                    <i class="icon-upload icon-white"></i>
                    <span>Sauvgarder</span>
                </button>
                <a href="/Admin/Direction" class="btn btn-warning cancel">Retour</a>
        </p>
</td></tr>
        </table>
}


