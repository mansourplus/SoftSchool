@model SoftSchool.Models.dres

@{
    ViewBag.Title = "Delete";
    Layout = Url.Content("~/Areas/Admin/Views/Shared/_Layout.cshtml");
}


<h4>Êtes-vous sûr de vouloir supprimer?</h4>
<fieldset>
   

    <div class="display-label">Nom_Ar</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Nom_Ar)
    </div>

    <div class="display-label">Nom_Fr</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Nom_Fr)
    </div>

    <div class="display-label">gouvernorats</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.gouvernorats.Nom_Ar)
    </div>
</fieldset>
<br /><br />
@using (Html.BeginForm()) {
    <p>
    <button type="submit" class="btn btn-danger delete" value="Delete" >
                    <i class="icon-upload icon-white"></i>
                    <span>Delete</span>
                </button>
        <a href="/Admin/Direction" class="btn btn-warning cancel">Retour</a>
    </p>
}
